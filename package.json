{
  "name": "forge-digital-catalog-app",
  "description": "An Autodesk Forge powered proof of concept digital catalog",
  "version": "1.0.2",
  "private": true,
  "author": "Bastien Mazeran <bastien.mazeran@autodesk.com> (https://www.linkedin.com/in/bastien-mazeran-01200414/)",
  "scripts": {
    "build": "npm run tsbuild && vue-cli-service build",
    "clean": "rm -rf www",
    "dev": "NODE_ENV=development nodemon src/server",
    "devtest": "jest --watch",
    "eslint": "npm run eslint:client && npm run eslint:config && npm run eslint:server && npm run eslint:shared && npm run eslint:test",
    "eslint:client": "eslint -c ./src/client/.eslintrc.json --ext .ts ./src/client",
    "eslint:config": "eslint -c ./config/.eslintrc.json --ext .ts ./config",
    "eslint:server": "eslint -c ./src/server/.eslintrc.json --ext .ts ./src/server",
    "eslint:shared": "eslint -c ./src/shared/.eslintrc.json --ext .ts ./src/shared",
    "eslint:test": "eslint -c ./__tests__/.eslintrc.json --ext .ts ./__tests__",
    "init": "node config/init.js",
    "lint": "vue-cli-service lint",
    "prestart": "npm run init",
    "serve": "NODE_ENV=development vue-cli-service serve",
    "setadmin": "node config/setAdmin.js",
    "start": "NODE_ENV=production node src/server",
    "test": "NODE_ENV=test jest",
    "tsbuild": "npm run tsbuild:client && npm run tsbuild:config && npm run tsbuild:server && npm run tsbuild:shared && npm run tsbuild:test",
    "tsbuild:client": "node node_modules/typescript/bin/tsc -p ./src/client",
    "tsbuild:config": "node node_modules/typescript/bin/tsc -p ./config",
    "tsbuild:server": "node node_modules/typescript/bin/tsc -p ./src/server",
    "tsbuild:shared": "node node_modules/typescript/bin/tsc -p ./src/shared",
    "tsbuild:test": "node node_modules/typescript/bin/tsc -p ./__tests__",
    "windev": "set NODE_ENV=development&& nodemon src/server",
    "winserve": "set NODE_ENV=development&& vue-cli-service serve",
    "winstart": "set NODE_ENV=production&& node src/server",
    "wintest": "set NODE_ENV=test&& jest"
  },
